{
  "manifest_version": 2,
  "minimum_chrome_version": "45",
  "name": "ShutUp!Tab",
  "description": "Mute the active tab permanently via black list of domains.",
  "version": "2.4.3",
  "author": "Gallyamov Ramil",
  "permissions": [
    "tabs",
    "storage",
    "contextMenus"
  ],
  "icons": {
    "128": "images/icon-app.png"
  },
  "browser_action": {
    "name": "ShutUp!Tab",
    "default_icon": "images/icon-normal.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
